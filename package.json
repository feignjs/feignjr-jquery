{
  "name": "feignjs-jquery",
  "version": "0.0.1",
  "description": "feignjs jquery client integration",
  "main": "feign-jquery.js",
  "scripts": {
    "test": "npm test",
    "bower": "npm run browserify && npm run uglify",
    "browserify": "node_modules/.bin/browserify feign-jquery.js -o dist/feign-jquery.js --s FeignJquery --no-bundle-external",
    "uglify": "node_modules/.bin/uglify dist/feign-jquery.js -o dist/feign-jquery.min.js",
    "prepublish": "npm run bower"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feignjs/feignjs-jquery.git"
  },
  "keywords": [
    "feignjs",
    "rest",
    "client",
    "declarative",
    "jquery"
  ],
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "args-js": "global:Args",
    "lodash": "global:_",
    "jquery": "global:jQuery"
  },
  "author": "Peter Mucha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feignjs/feignjs-jquery/issues"
  },
  "homepage": "https://github.com/feignjs/feignjs-jquery#readme",
  "dependencies": {
    "args-js": "^0.10.6",
    "jquery": "^2.1.4",
    "lodash": "^3.9.3"
  }
}
